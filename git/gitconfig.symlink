# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
	autocrlf = input

[init]
	defaultBranch = master
[push]
	default = simple
[safe]
	directory = /opt/flutter
[pull]
	rebase = true
